window.addEventListener('keydown', doKeyDown, true);
var canvas = document.getElementById("canvas");
alert('fae');
var ctx = canvas.getContext("2d");
var rect = new Rect(ctx,100,100,30,30);
function doKeyDown(e) {
    if ( e.keyCode == 87 ) {
        rect.move(0,-10);
    }
    if ( e.keyCode == 83 ) {
        rect.move(0,10);
    }
    if ( e.keyCode == 65 ) {
        rect.move(-10,0);
    }
    if ( e.keyCode == 68 ) {
        rect.move(10,0);
    }
}
